extends layout

block content
  h1 This is the profile page

  p User kits
  .container
    each kit in userKits
     .col-xs-3(style="margin-top:10px;")
       a(href="kit/" + kit['_id'])
         div(style="border: 1px solid black; margin:10px padding:10px")
           p Title: #{kit.title}
           p Intro: #{kit.content}

